getColor(el)\"></div>\n            </div>\n        </center>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: 'dl-inst-color',\n        methods: {\n            getColor (color) {\n                this.$store.commit('upColorChoose');\n                this.$store.commit('upColor', color)\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .chooseColor {\n        transform: rotate(90deg);\n    }\n    center {\n        position: relative;\n    }\n    .color {\n        width: 40px;\n        height: 40px;\n        border-radius: 20px;\n        cursor: pointer;\n        position: absolute;\n        top: 0;\n        margin-left: auto;\n        margin-right: auto;\n        left: 0;\n        right: 0;\n        z-index: 10;\n        transition: .2s;\n        border: 1px solid transparent;\n    }\n    .color:hover {\n        border: 1px solid rgba(150,150,150,.7);\n    }\n    .getColor {\n        position: absolute;\n        top: 100px;\n        width: 50px;\n        height: 185px;\n        background: rgba(150,150,150,.7);\n        margin-left: auto;\n        margin-right: auto;\n        left: 0;\n        right: 0;\n        border-radius: 25px;\n    }\n</style>"]}]}