        <dl-instruments v-if=\"side === 'right' && width > 159\" />\n        <dl-task v-if=\"side === 'left' && width > 159\" />\n        <div class=\"circleMenu left\" v-if=\"width < 160\"></div>\n        <div class=\"circleMenu right\" v-if=\"width < 160\"></div>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: 'dl-panel',\n        components: {\n            'dl-instruments': () => import('./instrument/dl-instruments.vue'),\n            'dl-task': () => import('./task/dl-task.vue')\n        },\n        props: {\n            side: String\n        },\n        data() {\n            return {\n                width: null,\n                height: null\n            }\n        },\n        methods: {\n            getWidth () {\n                return ((document.body.offsetWidth - document.body.offsetHeight)/2) - 10\n            },\n            getHeight () {\n                return document.body.offsetHeight - 20\n            },\n            onResize () {\n                this.width = this.getWidth();\n                this.height = this.getHeight()\n            }\n        },\n        mounted () {\n            window.addEventListener('resize', this.onResize);\n            this.onResize()\n        },\n    }\n</script>\n\n<style scoped>\n    .mainPanel {\n        position: absolute;\n        top: 10px;\n        height: calc(100vh - 20px);\n        animation: fade 1.5s 1;\n    }\n    @keyframes fade {\n        0%{\n            opacity: 0;\n        }\n        100%{\n            opacity: 1;\n        }\n    }\n    .circleMenu {\n        width: 40px;\n        height: 40px;\n        position: absolute;\n        top: 10px;\n        border-radius: 50%;\n        background: #ff3986;\n\n        cursor: pointer;\n    }\n    .left {\n        left: 10px;\n    }\n    .right {\n        right: 10px;\n    }\n</style>"]}]}